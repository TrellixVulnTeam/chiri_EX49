{
  "name": "@cornershop/cs-npm-s3",
  "version": "1.3.0",
  "description": "Publish npm packages to a Cornershop S3 bucket",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/cornershop/cornershop-s3-npm"
  },
  "author": "Rafael Poveda @rafa",
  "license": "MIT",
  "bin": {
    "cs-npm-s3": "./lib/bin/npm-s3.js"
  },
  "scripts": {
    "build": "tsc",
    "dev-publish": "ts-node ./src/bin/npm-s3.ts publish",
    "dev-info": "ts-node ./src/bin/npm-s3.ts info @cornershop/cs-npm-s3",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "test": "jest --config jest.config.json"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.0.26",
    "@types/npm": "^2.0.31",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@types/semver": "^7.3.1",
    "aws-sdk": "^2.720.0",
    "chalk": "^4.1.0",
    "npm": "^6.14.7",
    "packagenpmjson": "^1.0.0",
    "prettier": "^2.0.5",
    "semver": "^7.3.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "yarn": "^1.22.4"
  }
}
